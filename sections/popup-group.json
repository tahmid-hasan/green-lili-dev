{
  "type": "custom.popups",
  "name": "t:sections.popup-group.name",
  "sections": {
    "newsletter-popup": {
      "type": "newsletter-popup",
      "disabled": true,
      "settings": {
        "disable_for_account_holders": true,
        "popup_seconds": 5,
        "popup_days": 30,
        "popup_title": "Sign up and save",
        "popup_text": "<p>Entice customers to sign up for your mailing list with discounts or exclusive offers. Include an image for extra impact.<\/p>",
        "image_position": "left",
        "show_social_icons": false,
        "enable_newsletter": true,
        "button_label": "Optional button",
        "button_link": ""
      }
    },
    "age-verification-popup": {
      "type": "age-verification-popup",
      "disabled": true,
      "settings": {
        "enable_test_mode": false,
        "blur_image": false,
        "heading": "Confirm your age",
        "text": "<p>Are you 18 years old or older?<\/p>",
        "decline_button_label": "No I'm not",
        "approve_button_label": "Yes I am",
        "decline_heading": "Come back when you're older",
        "decline_text": "<p>Sorry, the content of this store can't be seen by a younger audience. Come back when you're older.<\/p>",
        "return_button_label": "Oops, I entered incorrectly"
      }
    },
    "localization-switcher": {
      "type": "localization-switcher",
      "settings": {
        "heading": "Select your location",
        "message": "Update the store settings for your location for a smoother checkout.",
        "country_label": "Select a country",
        "language_label": "Select a language",
        "button_label": "Confirm",
        "country_exclude": ""
      }
    }
  },
  "order": [
    "newsletter-popup",
    "age-verification-popup",
    "localization-switcher"
  ]
}
